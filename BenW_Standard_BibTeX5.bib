@book{Colliot-Thelene2021,
  title = {The {{Brauer-Grothendieck Group}}},
  author = {{Colliot-Th{\'e}l{\`e}ne}, Jean-Louis and Skorobogatov, Alexei N. and {Springer Mathematics and Statistics eBooks 2021 English/International}},
  year = {2021},
