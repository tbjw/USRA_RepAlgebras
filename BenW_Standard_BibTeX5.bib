@article{Druzhinin2020,
  title = {Stable Connectivity over a Base},
  author = {Druzhinin, A.},
  year = {2020},
  month = jan,
  journal = {arXiv:1911.05014 [math]},
  eprint = {1911.05014},
  primaryclass = {math},
  urldate = {2021-08-17},
  abstract = {Morel's stable connectivity theorems state that for any connective \$S\^1\$-spectrum \$F\$ of motivic spaces (Nisnevich simplicial sheaves) over an arbitrary field, the spectrum \$L\_\{\textbackslash mathbb A\^1\}(F)\$ is connective, and the same property for \$\textbackslash mathbb P\^1\$-spectra of motivic spaces. Here \$L\_\{\textbackslash mathbb A\^1\}\$ denotes the \$\textbackslash mathbb A\^1\$-localisation in the category of motivic spectra over a field \$k\$. Originally the same property was conjectured for the case of motivic \$S\^1\$-spectra over a base scheme \$S\$.In view of Ayoub's conterexamples the modified version of conjecture states that \$L\_\{\textbackslash mathbb A\^1\}(F)\$ is \$(-d)\$-connective for any connective \$F\$, where \$d=\textbackslash mathrm\{dim\} S\$ is the Krull dimension. The conjecture is proven under the infiniteness assumption on the residue fields for the cases of Dedekind schemes by J.\textasciitilde Schmidt and F.\textasciitilde Strunk and noetherian domains of arbitrary dimension by N.\textasciitilde Deshmukh, A.\textasciitilde Hogadi, G.\textasciitilde Kulkarni and S.\textasciitilde Yadavand. In the article we prove the result or general base with out the assumption on the residue fields. So by the result for any smooth scheme \$X\$ over a base scheme \$S\$ of Krull dimension \$d\$ the Nisnevich sheaves of \$S\^1\$-stable motivic homotopy groups \$\textbackslash pi\_i\^\{S\^1\}(X)\$ and \$\textbackslash mathbb P\^1\$-stable motivic homotopy groups \$\textbackslash pi\_\{i+j,j\}\^\{\textbackslash mathbb P\^1\}(X)\$ vanishes for all \$i{$<$}-d\$.},
  archiveprefix = {arxiv},
  keywords = {14F42; 55Q10,Mathematics - Algebraic Geometry}
}

