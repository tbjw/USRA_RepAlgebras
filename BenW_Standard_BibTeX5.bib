  year = {2017},
  month = jul,
  journal = {Journal of Pure and Applied Algebra},
  series = {Special Issue Dedicated to {{Chuck Weibel}} on the Occasion of His 60th Birthday.},
  volume = {221},
  number = {7},
  pages = {1729--1844},
  issn = {0022-4049},
  doi = {10.1016/j.jpaa.2016.12.026},
  urldate = {2022-03-24},
  abstract = {Within the framework of dg categories with weak equivalences and duality that have uniquely 2-divisible mapping complexes, we show that higher Grothendieck\textendash Witt groups (aka. hermitian K-groups) are invariant under derived equivalences and that Morita exact sequences induce long exact sequences of Grothendieck\textendash Witt groups. This implies an algebraic Bott sequence and a new proof and generalisation of Karoubi's Fundamental Theorem. For the higher Grothendieck\textendash Witt groups of vector bundles of (possibly singular) schemes X with an ample family of line-bundles such that 12{$\in\Gamma$}(X,OX), we obtain Mayer\textendash Vietoris long exact sequences for Nisnevich coverings and blow-ups along regularly embedded centres, projective bundle formulas, and a Bass fundamental theorem. For coherent Grothendieck\textendash Witt groups, we obtain a localization theorem analogous to Quillen's K{${'}$}-localization theorem.},
  langid = {english}
}

