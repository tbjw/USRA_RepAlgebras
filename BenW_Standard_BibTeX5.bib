@article{Barge2000,
  title = {Groupe de {{Chow}} Des Cycles Orient\'es et Classe d'{{Euler}} Des Fibr\'es Vectoriels},
  author = {Barge, Jean and Morel, Fabien},
  year = {2000},
  month = feb,
  journal = {Comptes Rendus de l'Acad\'emie des Sciences - Series I - Mathematics},
  volume = {330},
  number = {4},
  pages = {287--290},
  issn = {0764-4442},
  doi = {10.1016/S0764-4442(00)00158-0},
  urldate = {2018-08-15},
  abstract = {R\'esum\'e Soient k un corps de caract\'eristique 1=2 et X une k-vari\'et\'e lisse de dimension n. \`A tout fibr\'e vectoriel sur X, orient\'e de dimension n, {$\xi$}, nous associons fonctoriellement une classe d'Euler e({$\xi$}){$\in$}HnZar(X;Jn), Jn d\'esignant un certain faisceau. Lorsque k est alg\'ebriquement clos, ce groupe de cohomologie s'identifie au groupe de Chow CHn(X) des 0-cycles et la classe d'Euler \`a la classe de Chern cn({$\xi$}). Dans cette Note nous \'etudions le probl\`eme de savoir si cette classe d'Euler est la seule obstruction, pour X affine, \`a l'existence d'un facteur direct trivial de rang un dans {$\xi$}. Let k be a field of characteristic not 2 and X be a smooth k-variety of dimension n. To any oriented n-dimensional vector bundle {$\xi$}, we functorially assign an Euler class e({$\xi$}){$\in$}HnZar(X;Jn), where Jn is a certain sheaf. When k is algebraically closed, this cohomology group reduces to the Chow group CHn(X) of 0-cycles and the Euler class to the Chern class cn({$\xi$}). In this Note we study the following problem: for X affine, is the vanishing of the Euler class the only obstruction for {$\xi$} to have a free direct summand of rank one.}
}

