  doi = {10.1016/j.aim.2017.08.034},
  urldate = {2020-09-20},
  abstract = {We improve homology stability ranges for elementary and special linear groups over rings with many units. Our result implies stability for unstable Quillen K-groups and proves a conjecture of Bass. For commutative local rings with infinite residue fields, we show that the obstruction to further stability is given by Milnor\textendash Witt K-theory. As an application we construct Euler classes of projective modules with values in the cohomology of the Milnor\textendash Witt K-theory sheaf. For d-dimensional commutative noetherian rings with infinite residue fields we show that the vanishing of the Euler class is necessary and sufficient for an oriented projective module P of rank d to split off a rank 1 free direct summand. Along the way we obtain a new presentation of Milnor\textendash Witt K-theory and of symplectic K2 simplifying the classical Matsumoto\textendash Moore presentation.},
  langid = {english},
  keywords = {Euler class of a projective module,Homology of special linear groups,Matsumoto–Moore Theorem,Milnor–Witt -theory,Schur multiplier,Stability in -theory}
}

@article{Schlichting2017a,
