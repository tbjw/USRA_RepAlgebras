@article{Asok2019,
  title = {{{OBSTRUCTIONS TO ALGEBRAIZING TOPOLOGICAL VECTOR BUNDLES}}},
  author = {Asok, A. and Fasel, J. and Hopkins, M. J.},
  year = {2019/ed},
  journal = {Forum of Mathematics, Sigma},
  volume = {7},
  publisher = {{Cambridge University Press}},
  issn = {2050-5094},
  doi = {10.1017/fms.2018.16},
  urldate = {2022-06-02},
  abstract = {Suppose {$\mathsl{X}$}XX is a smooth complex algebraic variety. A necessary condition for a complex topological vector bundle on {$\mathsl{X}$}XX (viewed as a complex manifold) to be algebraic is that all Chern classes must be algebraic cohomology classes, that is, lie in the image of the cycle class map. We analyze the question of whether algebraicity of Chern classes is sufficient to guarantee algebraizability of complex topological vector bundles. For affine varieties of dimension {$\leqslant$}3{$\leqslant$}3\{\textbackslash leqslant\}3, it is known that algebraicity of Chern classes of a vector bundle guarantees algebraizability of the vector bundle. In contrast, we show in dimension {$\geqslant$}4{$\geqslant$}4\{\textbackslash geqslant\}4 that algebraicity of Chern classes is insufficient to guarantee algebraizability of vector bundles. To do this, we construct a new obstruction to algebraizability using Steenrod operations on Chow groups. By means of an explicit example, we observe that our obstruction is nontrivial in general.},
  langid = {english},
  keywords = {13C10 (secondary),14F42 (primary),32L05,55R25}
}

